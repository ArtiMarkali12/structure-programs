#include<stdio.h>
struct emp
{
        int eid;
        char ename[20];
        float sal;
        }e1[100];
        int main()
        {
        int i,n;
        printf("enter limit:");
        scanf("%d",&n);
        for(i=0;i<n;i++)
        {
        printf("enter emp no name sal:");
        scanf("%d%s%f",&e1[i].eid,&e1[i].ename,&e1[i].sal);
        }

        for(i=0;i<n;i++)
        {

        printf("\n emp id =%d \n emp name =%s \n emp sal=%f",e1[i].eid,e1[i].ename,e1[i].sal); 
        }
        }

